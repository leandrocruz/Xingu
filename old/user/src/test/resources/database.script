CREATE SEQUENCE USER_ID_SEQ AS BIGINT START WITH 100 
CREATE TABLE USER(ID BIGINT NOT NULL PRIMARY KEY,NAME VARCHAR(128),USERNAME VARCHAR(64) NOT NULL,PASSWORD VARCHAR(32),IS_ACTIVE BOOLEAN,LIST_ID BIGINT,REGISTRATION DATE,CONFIRMED BOOLEAN,CONFIRMATION_CODE VARCHAR(32),COMPLETE BOOLEAN)
CREATE UNIQUE INDEX USERNAME_IDX ON USER(USERNAME)
CREATE TABLE ID_TABLE(ID INTEGER NOT NULL PRIMARY KEY,GENERATOR_ID VARCHAR NOT NULL,LAST_USED BIGINT NOT NULL)
CREATE TABLE ATTRIBUTE(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 100)  NOT NULL PRIMARY KEY,LIST_ID INTEGER NOT NULL,TYPE VARCHAR NOT NULL,VALUE VARCHAR)
CREATE TABLE ATTRIBUTE_LIST(ID INTEGER NOT NULL PRIMARY KEY,NAMESPACE VARCHAR)
